-Comment (\/\*(.|\s)*\*\/)|(\/\/(.)*\n)
IF if
VAR var
PRINT print
NUMBER ([0-9]+)*(\.)?[0-9]*
WHILE while
ELSE else
ID [a-zA-Z][a-zA-Z0-9_]*
STRING \"[^\"]*\"
ESCAPE_STRING \"([^\"]|(\\[n\"])+)*\"
SCOPE_Start \{
NOT_STRING \"[^\"]*
SCOPE_END \}
OPEN_PARENTHESIS \(
MATH "**"|[+\-*/%]
CLOSE_PARENTHESIS \)
EQUALS "=="|"!="
COMPARE "<"|">"|"<="|">="
BOOLEAN_OP "&&"|"||"
ASSIGN =
-WHITESPACE \s
ENDLINE ;
UNKNOWN .
